<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/mdui/0.4.2/css/mdui.min.css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/mdui/0.4.2/js/mdui.min.js"></script>

    <meta charset="UTF-8">
    <title>admin</title>

</head>
<body>
<!-- Simple header with scrollable tabs. -->
<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header mdl-layout--no-drawer-button">
    <header class="mdl-layout__header">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <div class="mdl-layout__header-row">
            <!-- Title -->
            <span class="mdl-layout-title">ADMIN</span>
            <div class="mdl-layout-spacer"></div>
            <!--navigation bar-->
            <nav class="mdl-navigation">
                <a class="mdl-navigation__link" href=""><i class="material-icons">account_circle</i> </a>
                <a class="mdl-navigation__link" href="/login"><i class="material-icons">power_settings_new</i> Sign out </a>
            </nav>
        </div>
        <!-- Tabs -->
        <div class="mdl-layout__tab-bar mdl-js-ripple-effect">
            <a href="#scroll-tab-1" class="mdl-layout__tab is-active">team management</a>
            <a href="../A2" class="mdl-layout__tab">judge management</a>
            <a href="../A3" class="mdl-layout__tab">add accounts</a>
            <a href="../A4" class="mdl-layout__tab">competition management</a>
        </div>
    </header>
    <!--drawer-->

    <main class="mdl-layout__content">
        <section class="mdl-layout__tab-panel is-active" id="scroll-tab-1">
                <div class="page-content">
<!--                    team selection and search-->
                    <form id="adminNum1">
                        <input type="hidden" name="adminNum" value="1">
                        <div class="mdl-grid">
                        <div class="mdl-layout-spacer"></div>
                        <div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--4-col">
                            <div class="mdl-card__title">
                                <h1 class="mdl-card__title-text">Team  Selection</h1>
                            </div>
                            <div class="mdl-card__supporting-text scrollable ">
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label" style="width: 100%;">
                                    <select class="mdl-textfield__input" id="team" name="team">
                                        <option></option>
                                        <option value="team1">Team 1</option>
                                        <option value="team2">Team 2</option>
                                        <option value="team3">Team 3</option>
                                        <option value="team4">Team 4</option>
                                    </select>
                                    <label class="mdl-textfield__label" for="team">Team</label>
                                </div>
                            </div>
                            <div class="mdl-card__actions">
                                <button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect">
                                    search
                                </button>
                            </div>
                        </div>
                        <div class="mdl-layout-spacer"></div>
                    </div>
                    </form>
                    <!-- team management goes here -->
                    <!-- team management goes here -->
                    <!-- team management goes here -->

<!--                    team one-->
                    <form action="">
                        <div class="mdl-grid">
                            <div class="mdl-layout-spacer"></div>
                            <div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--8-col">
                                <div class="mdl-card__title">
                                    <h1 class="mdl-card__title-text">Team 1</h1>
                                </div>
                                <div class="mdl-card__supporting-text scrollable">
                                    <h5>Basic Information</h5>
                                    <div class="mdui-table-fluid">
                                        <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp" width="100%">
                                            <tbody>
                                            <tr>
                                                <td>Team Name</td>
                                                <td><input type="text" name="teamName" value="team1"></td>
                                            </tr>
                                            <tr>
                                                <td>Manager</td>
                                                <td><input type="text" name="manager" value="manager"></td>
                                            </tr>
                                            <tr>
                                                <td>Phone</td>
                                                <td><input type="tel" name="phoneNum" value="1111"></td>
                                            </tr>
                                            <tr>
                                                <td>E-mail</td>
                                                <td><input type="email" name="eMail" value="111@qq.qqq"></td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="mdl-card__supporting-text scrollable">
                                    <h5>Result Summary</h5>
                                    <div class="mdui-table-fluid">
                                        <table class="mdui-table" width="100%">
                                            <thead>
                                            <tr>
                                                <th>Challenges</th>
                                                <th>Attempts</th>
                                                <th>Time</th>
                                                <th>Additional Points</th>
                                                <th>Total Scores</th>
                                            </tr>
                                            </thead>
                                            <tbody>
                                            <tr>
                                                <td rowspan="3">The Hubble Telescope Nebula Challenge</td>
                                                <td>attempt 1</td>
                                                <td><input type="number" name="C1_1_time"></td>
                                                <td><input type="number" name="C1_1_reward"></td>
                                                <td rowspan="3"><input type="number" name="C1score"></td>
                                            </tr>
                                            <tr>
                                                <td>attempt 2</td>
                                                <td><input type="number" name="C1_2_time"></td>
                                                <td><input type="number" name="C1_2_reward"></td>
                                            </tr>
                                            <tr>
                                                <td>attempt 3</td>
                                                <td><input type="number" name="C1_3_time"></td>
                                                <td><input type="number" name="C1_3_reward"></td>
                                            </tr>
                                            <tr>
                                                <td rowspan="3">The Canyons of Mars</td>
                                                <td>1st attempt</td>
                                                <td><input type="number" name="C2_1_time"></td>
                                                <td><input type="number" name="C2_1_reward"></td>
                                                <td rowspan="3"><input type="number" name="C2score"></td>
                                            </tr>
                                            <tr>
                                                <td>2nd attempt</td>
                                                <td><input type="number" name="C2_2_time"></td>
                                                <td><input type="number" name="C2_2_reward"></td>
                                            </tr>
                                            <tr>
                                                <td>3rd attempt</td>
                                                <td><input type="number" name="C2_3_time"></td>
                                                <td><input type="number" name="C2_3_reward"></td>
                                            </tr>
                                            <tr>
                                                <td rowspan="3">Blast Off: the Straight-ish Line Speed Test</td>
                                                <td>1st attempt</td>
                                                <td><input type="number" name="C3_1_time"></td>
                                                <td><input type="number" name="C3_1_reward"></td>
                                                <td rowspan="3"><input type="number" name="C3score"></td>
                                            </tr>
                                            <tr>
                                                <td>2nd attempt</td>
                                                <td><input type="number" name="C3_2_time"></td>
                                                <td><input type="number" name="C3_2_reward"></td>
                                            </tr>
                                            <tr>
                                                <td>3rd attempt</td>
                                                <td><input type="number" name="C3_3_time"></td>
                                                <td><input type="number" name="C3_3_reward"></td>
                                            </tr>

                                            <tr>
                                                <td>Pi Noon: The Right Stuff</td>
                                                <td colspan="2">Number of wins: <input type="number" name="C4_win"></td>
                                                <td><input type="number" name="C4_reward"></td>
                                                <td><input type="number" name="C4score"></td>
                                            </tr>
                                            <tr>
                                                <td rowspan="3">Space Invaders</td>
                                                <td>1st attempt</td>
                                                <td><input type="number" name="C5_1_time"></td>
                                                <td><input type="number" name="C5_1_reward"></td>
                                                <td rowspan="3"><input type="number" name="C5score"></td>
                                            </tr>
                                            <tr>
                                                <td>2nd attempt</td>
                                                <td><input type="number" name="C5_2_time"></td>
                                                <td><input type="number" name="C5_2_reward"></td>
                                            </tr>
                                            <tr>
                                                <td>3rd attempt</td>
                                                <td><input type="number" name="C5_3_time"></td>
                                                <td><input type="number" name="C5_3_reward"></td>
                                            </tr>
                                            <tr>
                                                <td>Spirit of Curiosity</td>
                                                <td colspan="3" style="text-align: right">total points:</td>
                                                <td><input type="number" name="C6score"></td>
                                            </tr>
                                            <tr>
                                                <td rowspan="3">The Apollo 13 Obstacle Course</td>
                                                <td>1st attempt</td>
                                                <td><input type="number" name="C7_1_time"></td>
                                                <td>66</td>
                                                <td rowspan="3"><input type="number" name="C7score"></td>
                                            </tr>
                                            <tr>
                                                <td>2nd attempt</td>
                                                <td><input type="number" name="C7_2_time"></td>
                                                <td>66</td>
                                            </tr>
                                            <tr>
                                                <td>3rd attempt</td>
                                                <td><input type="number" name="C7_3_time"></td>
                                                <td>22</td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="mdl-card__supporting-text scrollable">
                                    <h5>Additional Points  By Expert Judges</h5>
                                    <div class="mdui-table-fluid">
                                        <table class="mdui-table">
                                            <tbody>
                                            <tr>
                                                <td style="width: 50%;">Additional Points  By Expert Judges</td>
                                                <td><input type="number" name="C8score"></td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="mdl-card__supporting-text scrollable">
                                    <h5>Final Score</h5>
                                    <div class="mdui-table-fluid">
                                        <table class="mdui-table">
                                            <td style="width: 50%;">Final score</td>
                                            <td><input type="number" name="Cscore"></td>
                                        </table>
                                    </div>
                                </div>
                                <div class="mdl-card__actions">
                                    <button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent">
                                        Confirm
                                    </button>
                                </div>
                            </div>
                            <div class="mdl-layout-spacer"></div>
                        </div>
                    </form>
                </div>
        </section>
        <footer class="mdl-mini-footer">
            <div class="mdl-mini-footer__left-section">
                <div class="mdl-logo"><a href="/privacy">Privacy & Terms</a></div>
            </div>
        </footer>
    </main>


</div>
</body>
</html>