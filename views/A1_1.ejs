<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Introducing Lollipop, a sweet new take on Android.">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">
    <title>Pi-wars</title>
    <link rel="icon" href="/images/title-logo.png" sizes="32x32">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/mdui/0.4.2/css/mdui.min.css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/mdui/0.4.2/js/mdui.min.js"></script>
    <script src="https://cdn.bootcss.com/jquery/3.3.1/jquery.min.js"></script>
    <script src="/javascripts/getA1Data.js"></script>
    <meta charset="UTF-8">
    <title>admin</title>
    <link rel="icon" href="/images/title-logo.png" sizes="32x32">
</head>
<!--team management searching results-->
<% for(var i = 0;i < result.length;i++){ %>
<form action="" id="A1Form2">
    <div class="mdl-grid">
        <div class="mdl-layout-spacer"></div>
        <div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--8-col">
            <div class="mdl-card__title">
                <h1 class="mdl-card__title-text"><input class="mdl-textfield__input" type="hidden" name="oldname"
                                                        value="<%= result[i].name %>"><%= result[i].name %></h1>
            </div>
            <div class="mdl-card__supporting-text scrollable">
                <h5>Basic Information</h5>
                <div class="mdui-table-fluid">
                    <table class="mdui-table">
                        <tbody>
                        <tr>
                            <td>Team Name</td>
                            <td><input class="mdl-textfield__input" name="name" type="text" value=<%= result[i].name %>></td>
                        </tr>
                        <tr>
                            <td>Manager</td>
                            <td><input class="mdl-textfield__input" name="manager" type="text" value=<%= result[i].manager %>></td>
                        </tr>
                        <tr>
                            <td>Phone</td>
                            <td><input class="mdl-textfield__input" name="phone_number" type="text" value=<%= result[i].phone_number %>></td>
                        </tr>
                        <tr>
                            <td>E-mail</td>
                            <td><input class="mdl-textfield__input" name="email" type="text" value=<%= result[i].email %>></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="mdl-card__supporting-text scrollable">
                <h5>Competition Information</h5>
                <div class="mdui-table-fluid">
                    <table class="mdui-table">
                        <thead>
                        <tr>
                            <td>Competition</td>
                            <td>Attempts</td>
                            <td>Time (s)</td>
                            <td>Additional Points</td>
                            <td>Total Scores</td>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td rowspan="3">The Hubble Telescope Nebula Challenge</td>
                            <td>1st attempt</td>
                            <td><input class="mdl-textfield__input" name="C1_1_time" type="text" value=<%= result[i].C1_1_time %>></td>
                            <td><input class="mdl-textfield__input" name="C1_1_reward" type="text" value=<%= result[i].C1_1_reward %>></td>
                            <td rowspan="3"><input class="mdl-textfield__input" name="C1score" type="text" value=<%= result[i].C1score %>></td>
                        </tr>
                        <tr>
                            <td>2nd attempt</td>
                            <td><input class="mdl-textfield__input" name="C1_2_time" type="text" value=<%= result[i].C1_2_time %>></td>
                            <td><input class="mdl-textfield__input" name="C1_2_reward" type="text" value=<%= result[i].C1_2_reward %>></td>
                        </tr>
                        <tr>
                            <td>3rd attempt</td>
                            <td><input class="mdl-textfield__input" name="C1_3_time" type="text" value=<%= result[i].C1_3_time %>></td>
                            <td><input class="mdl-textfield__input" name="C1_3_reward" type="text" value=<%= result[i].C1_3_reward %>></td>
                        </tr>
                        <tr>
                            <td rowspan="3">The Canyons of Mars</td>
                            <td>1st attempt</td>
                            <td><input class="mdl-textfield__input" name="C2_1_time" type="text" value=<%= result[i].C2_1_time %>></td>
                            <td><input class="mdl-textfield__input" name="C2_1_reward" type="text" value=<%= result[i].C2_1_reward %>></td>
                            <td rowspan="3"><input class="mdl-textfield__input" name="C2score" type="text" value=<%= result[i].C2score %>></td>
                        </tr>
                        <tr>
                            <td>2nd attempt</td>
                            <td><input class="mdl-textfield__input" name="C2_2_time" type="text" value=<%= result[i].C2_2_time %>></td>
                            <td><input class="mdl-textfield__input" name="C2_2_reward" type="text" value=<%= result[i].C2_2_reward %>></td>
                        </tr>
                        <tr>
                            <td>3rd attempt</td>
                            <td><input class="mdl-textfield__input" name="C2_3_time" type="text" value=<%= result[i].C2_3_time %>></td>
                            <td><input class="mdl-textfield__input" name="C2_3_reward" type="text" value=<%= result[i].C2_3_reward %>></td>
                        </tr>

                        <tr>
                            <td rowspan="3">Blast Off: the Straight-ish Line Speed Test</td>
                            <td>1st attempt</td>
                            <td><input class="mdl-textfield__input" name="C3_1_time" type="text" value=<%= result[i].C3_1_time %>></td>
                            <td><input class="mdl-textfield__input" name="C3_1_reward" type="text" value=<%= result[i].C3_1_reward %>></td>
                            <td rowspan="3"><input class="mdl-textfield__input" name="C3score" type="text" value=<%= result[i].C3score %>></td>
                        </tr>
                        <tr>
                            <td>2nd attempt</td>
                            <td><input class="mdl-textfield__input" name="C3_2_time" type="text" value=<%= result[i].C3_2_time %>></td>
                            <td><input class="mdl-textfield__input" name="C3_2_reward" type="text" value=<%= result[i].C3_2_reward %>></td>
                        </tr>
                        <tr>
                            <td>3rd attempt</td>
                            <td><input class="mdl-textfield__input" name="C3_3_time" type="text" value=<%= result[i].C3_3_time %>></td>
                            <td><input class="mdl-textfield__input" name="C3_3_reward" type="text" value=<%= result[i].C3_3_reward %>></td>
                        </tr>

                        <tr>
                            <td>Pi Noon: The Right Stuff</td>
                            <td colspan="2">Number of wins: <input class="mdl-textfield__input" name="C4_win" type="text"
                                                                   value=<%= result[i].C4_win %>></td>
                            <td><input class="mdl-textfield__input" name="C4_reward" type="text" value=<%= result[i].C4_reward %>></td>
                            <td><input class="mdl-textfield__input" name="C4score" type="text" value=<%= result[i].C4score %>></td>
                        </tr>
                        <tr>
                            <td rowspan="3">Space Invaders</td>
                            <td>1st attempt</td>
                            <td><input class="mdl-textfield__input" name="C5_1_time" type="text" value=<%= result[i].C5_1_time %>></td>
                            <td><input class="mdl-textfield__input" name="C5_1_reward" type="text" value=<%= result[i].C5_1_reward %>></td>
                            <td rowspan="3"><input class="mdl-textfield__input" name="C5score" type="text" value=<%= result[i].C5score %>></td>
                        </tr>
                        <tr>
                            <td>2nd attempt</td>
                            <td><input class="mdl-textfield__input" name="C5_2_time" type="text" value=<%= result[i].C5_2_time %>></td>
                            <td><input class="mdl-textfield__input" name="C5_2_reward" type="text" value=<%= result[i].C5_2_reward %>></td>
                        </tr>
                        <tr>
                            <td>3rd attempt</td>
                            <td><input class="mdl-textfield__input" name="C5_3_time" type="text" value=<%= result[i].C5_3_time %>></td>
                            <td><input class="mdl-textfield__input" name="C5_3_reward" type="text" value=<%= result[i].C5_3_reward %>></td>
                        </tr>
                        <tr>
                            <td>Spirit of Curiosity</td>
                            <td colspan="3" style="text-align: right">total score</td>
                            <td><input class="mdl-textfield__input" name="C6score" type="text" value=<%= result[i].C6score %>></td>
                        </tr>
                        <tr>
                            <td rowspan="3">The Apollo 13 Obstacle Course</td>
                            <td>1st attempt</td>
                            <td><input class="mdl-textfield__input" name="C7_1_time" type="text" value=<%= result[i].C7_1_time %>></td>
                            <td><input class="mdl-textfield__input" name="C7_1_reward" type="text" value=<%= result[i].C7_1_reward %>></td>
                            <td rowspan="3"><input class="mdl-textfield__input" name="C7score" type="text" value=<%= result[i].C7score %>></td>
                        </tr>
                        <tr>
                            <td>2nd attempt</td>
                            <td><input class="mdl-textfield__input" name="C7_2_time" type="text" value=<%= result[i].C7_2_time %>></td>
                            <td><input class="mdl-textfield__input" name="C7_2_reward" type="text" value=<%= result[i].C7_2_reward %>></td>
                        </tr>
                        <tr>
                            <td>3rd attempt</td>
                            <td><input class="mdl-textfield__input" name="C7_3_time" type="text" value=<%= result[i].C7_3_time %>></td>
                            <td><input class="mdl-textfield__input" name="C7_3_reward" type="text" value=<%= result[i].C7_3_reward %>></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="mdl-card__supporting-text scrollable">
                <h5>Technical Scores</h5>
                <div class="mdui-table-fluid">
                    <table class="mdui-table">
                        <tbody>
                        <tr>
                            <td style="width: 50%;">Technical Scores</td>
                            <td><input class="mdl-textfield__input" name="C8score" type="text" value=<%= result[i].C8score %>></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="mdl-card__supporting-text scrollable">
                <h5>Final Score</h5>
                <div class="mdui-table-fluid">
                    <table class="mdui-table">
                        <td style="width: 50%;">Final score</td>
                        <td><input class="mdl-textfield__input" name="Cscore" type="text" value=<%= result[i].Cscore %>></td>
                    </table>
                </div>
            </div>
            <div class="mdl-card__actions">
                <button type="submit" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent">
                    Confirm
                </button>
            </div>
        </div>
        <div class="mdl-layout-spacer"></div>
    </div>
</form>
<% } %>
