<head>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/mdui/0.4.2/css/mdui.min.css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/mdui/0.4.2/js/mdui.min.js"></script>
    <script src="https://cdn.bootcss.com/jquery/3.3.1/jquery.min.js"></script>
    <script src="/javascripts/getA2Data.js"></script>
    <meta charset="UTF-8">
    <title>admin</title>
    <link rel="icon" href="/images/title-logo.png" sizes="32x32">

</head>
<form id="A2Form2" action="">
<% for(var i = 0;i < result.length;i++){ %>
    <div class="mdl-grid">
        <div class="mdl-layout-spacer"></div>
        <div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--8-col">
            <div class="mdl-card__title">
                <h1 class="mdl-card__title-text"><input type="hidden" name="oldname"
                                                        value="<%= result[i].name %>"><%= result[i].name %></h1>
            </div>
            <div class="mdl-card__supporting-text scrollable">
                <div class="mdui-table-fluid">
                    <table class="mdui-table">
                        <thead>
                        <tr>
                            <td>Name</td>
                            <td>Password</td>
                            <td>Permission</td>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td><input type="text" name="name" value=<%= result[i].name %>></td>
                            <td><input type="text" name="password" value=<%= result[i].password %>></td>
                            <td><input type="text" name="permission" value=<%= result[i].permission %>></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="mdl-card__actions">
                <button type="submit" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent">
                    Confirm
                </button>
            </div>
        </div>
        <div class="mdl-layout-spacer"></div>
    </div>
<% } %>
</form>