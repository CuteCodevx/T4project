        <!--login dialog-->


        <footer class="android-footer mdl-mega-footer">
          <div class="mdl-mega-footer--bottom-section">
            <a class="android-link mdl-typography--font-light" href="/privacy">Privacy Policy</a>
          </div>
        </footer>
